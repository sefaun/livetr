{
  "name": "livetr",
  "version": "1.0.0",
  "author": "SEFA UN",
  "description": "This is Livetr Desktop App",
  "private": true,
  "type": "commonjs",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "electron": "node scripts/startElectron.js",
    "setup": "electron-builder",
    "set-commonjs": "node scripts/setCommonJS.js",
    "set-module": "node scripts/setModule.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@tailwindcss/vite": "^4.0.13",
    "element-plus": "^2.9.6",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "sass": "^1.87.0",
    "tailwindcss": "^4.0.13",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^35.0.1",
    "electron-builder": "^24.13.3",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  },
  "build": {
    "electronVersion": "35.0.1",
    "appId": "com.livetr.id",
    "productName": "Livetr",
    "directories": {
      "output": "release"
    },
    "win": {
      "icon": "public/icon-260x260.png",
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "icon": "public/icon-260x260.png",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "icon": "public/icon-260x260.png",
      "target": [
        "AppImage"
      ]
    }
  }
}
